{
  "name": "tauri-svelte-pro",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "test": "vitest run",
    "test:ui": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.svelte",
    "fmt": "prettier --write .",
    "check": "tsc --noEmit && svelte-check --tsconfig ./tsconfig.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky && playwright install --with-deps || true",
    "commit": "cz",
    "release": "release-please release-pr",
    "dev:workspace": "turbo run dev",
    "build:workspace": "turbo run build",
    "test:workspace": "turbo run test",
    "lint:workspace": "turbo run lint",
    "fmt:workspace": "turbo run fmt"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tauri-apps/cli": "^1.6.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/svelte": "^5.2.6",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.6.0",
    "@storybook/addon-essentials": "^8.1.10",
    "@storybook/addon-interactions": "^8.1.10",
    "@storybook/svelte": "^8.1.10",
    "@storybook/svelte-vite": "^8.1.10",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "msw": "^2.3.1",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "svelte": "^4.2.15",
    "svelte-check": "^3.6.9",
    "tailwindcss": "^3.4.7",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.3.3",
    "vitest": "^1.6.0",
    "zod": "^3.23.8",
    "@playwright/test": "^1.46.0",
    "storybook": "^8.1.10",
    "cz-git": "^1.9.2"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.3"
  },
  "lint-staged": {
    "*.{ts,svelte,js,cjs,mjs}": [
      "eslint --fix"
    ],
    "*.{ts,js,json,css,md,svelte}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-git"
    }
  }
}
